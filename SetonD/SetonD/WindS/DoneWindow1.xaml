<Window x:Class="SetonD.WindS.DoneWindow1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SetonD.WindS"
        mc:Ignorable="d"
        Title="Window1" Height="500" Width="500">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"
                    Height="450" Width="200">
            <TextBlock x:Name="Welcome" VerticalAlignment="Center" HorizontalAlignment="Center"
                       Margin="0,100,0,0" FontSize="30" Text="Добро пожаловать в систему User" TextWrapping="Wrap"
                       TextAlignment="Center"/>

            <Button VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,100,0,200"
                    Height="40" Width="150" Content="Назад" FontSize="20" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
